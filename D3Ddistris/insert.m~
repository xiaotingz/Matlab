function outCell = insert(inCell, toAddList)
% ############################################################################
% NOTES
% - This file is a sub-function for findQuadPoints
% ############################################################################
% ----------------------- load debug data -----------------------
% a = {{1,2,3};{4};{}};
% inCell = a{2};
% toAddList = [1,2];
% ---------------------------------------------------------------
inCell = cell2mat(inCell);
for i = 1:length(toAddList)
    if ismember(toAddList(i), inCell)
        outCell = inCell;
    else
        outCell = [inCell, toAddList(i)];
        outCell = num2cell(outCell);
    end
end

end

